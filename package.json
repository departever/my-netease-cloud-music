{
  "name": "cloudmusic",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "prepare": "husky",
    "electron:start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "prettier --write",
      "eslint --max-warnings=999"
    ]
  },
  "dependencies": {
    "@better-scroll/core": "^2.5.1",
    "@better-scroll/mouse-wheel": "^2.5.1",
    "@better-scroll/scroll-bar": "^2.5.1",
    "axios": "^1.6.8",
    "element-plus": "^2.6.1",
    "good-storage": "^1.1.1",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "xgplayer": "^2.1.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "autoprefixer": "^10.4.19",
    "electron": "^34.3.0",
    "electron-builder": "^25.1.8",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.6.1",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss-pxtorem": "^6.1.0",
    "prettier": "^3.6.2",
    "sass": "^1.72.0",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.6"
  },
  "build": {
    "appId": "com.example.musicapp",
    "productName": "MusicApp",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "src/main/**/*",
      "public/music.png"
    ],
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis",
      "icon": "public/music.png"
    },
    "linux": {
      "target": "deb"
    }
  },
  "main": "src/main/main.cjs"
}
